{
  "env": {
    "browser": true,
    "es2021": true,
    "jquery": true
  },
  "globals": {
    "M": "readonly"
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "script"
  },
  "rules": {
    "no-console": "off",
    "no-unused-vars": [
      "warn",
      {
        "argsIgnorePattern": "^_"
      }
    ]
  }
}


